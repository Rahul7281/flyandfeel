{
  "version": 2,
  "builds": [
    {
      "src": "flyandfeel/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15gb",
        "runtime": "python3.10"  // Change this to python3.10 or python3.11
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "flyandfeel/wsgi.py"
    }
  ]
}
